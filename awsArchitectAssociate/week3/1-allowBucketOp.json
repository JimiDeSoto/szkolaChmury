{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "listBucketObj",
            "Effect": "Allow",
            "Action": ["s3:ListBucket"],
            "Resource": ["arn:aws:s3:::hw-week3"]
        },
        {
            "Sid": "getPutObj",
            "Effect": "Allow",
            "Action":[
                "s3:PutObject",
                "s3:GetObject"
            ],
            "Resource": ["arn:aws:s3:::hw-week3/*"]
        }
    ]
}